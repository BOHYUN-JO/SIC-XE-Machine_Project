/* how to compile */

# make

/* how to run */

# ./20160641.out

/* command list */

help : print help message

d[ir] : print file list in curren directory

hi[story] : print command history

du[mp] : show 160  memory spaces 
du[mp] start : show 160 memory spaces from start.
du[mp] start, end : show 160 memory spaces from start to the end

e[dit] address, value : edit memory that address points to value

f[ill] start, end, value : fill memory spaces to value from start to the end

reset : reset all memory spaces

opcode mnemonic : print mnemonic`s opcode 

opcodelist : print opcode hashtable to appropriate format

q[uit] : quit the program

assemble "filename" : assemble file

type "filename" : print contents of file

symbol : print symbol table alphabetically

progaddr "address" : set start of program addresss

loader "filename" : link and load the program

bp "break point" : set break point for debugging

bp clear : clear all break points

bp : print all break points

run : run the program
